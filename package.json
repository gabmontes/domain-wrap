  "keywords": [
    "async",
    "asynchronous",
    "deprecated",
    "domains",
    "error",
    "function",
    "handling",
    "protect",
    "wrap"
  ],
  "bugs": {
    "url": "https://github.com/gabmontes/domain-wrap/issues"
  },
  "license": "WTFPL",
  "author": "Gabriel Montes <gabriel@utoctadel.com.ar>",
  "files": [
    "dist",
    "lib"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabmontes/domain-wrap.git"
  },
  "scripts": {
