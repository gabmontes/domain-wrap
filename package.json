    "babel-preset-latest": "^6.16.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.0.0",
    "eslint": "^6.7.0",
    "mocha": "^3.1.2",
    "nyc": "^11.0.0"
  },
  "homepage": "https://github.com/gabmontes/domain-wrap#readme",
  "directories": {
